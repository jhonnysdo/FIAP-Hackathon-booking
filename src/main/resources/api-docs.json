            "type": "integer",
            "format": "int64"
          },
          "tipo": {
            "type": "string"
          },
          "totalPessoas": {
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "totalCamas": {
            "minimum": 1,
            "type": "integer",
            "format": "int32"
          },
          "outrosMoveis": {
            "type": "string"
          },
          "valorDiaria": {
            "type": "number",
            "format": "double"
          },
          "predioId": {
            "type": "integer",
            "format": "int64"
          },
          "blockedByAdmin": {
            "type": "boolean"
          }
        }
      },
