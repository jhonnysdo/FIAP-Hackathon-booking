        "required": [
          "nome",
          "tipo",
          "valor"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "nome": {
            "type": "string"
          },
          "valor": {
            "minimum": 1,
            "type": "number",
            "format": "double"
          },
          "tipo": {
            "type": "string",
            "enum": [
              "SERVICO",
              "ITEM"
            ]
          }
        }
      },
      "ReservaDTO": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
