          "endereco",
          "nomeCompleto",
          "paisDeOrigem",
          "sexo",
          "telefone"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "nomeCompleto": {
            "maxLength": 50,
            "minLength": 2,
            "pattern": "^[a-zA-Z]+(([',. -][a-zA-Z ])?[a-zA-Z]*)*$",
            "type": "string"
          },
          "dataNascimento": {
            "type": "string",
            "format": "date"
          },
          "cpf": {
            "type": "string"
          },
          "sexo": {
            "type": "string",
            "enum": [
              "MASCULINO",
              "FEMININO",
              "OUTRO"
            ]
          },
          "email": {
            "type": "string"
          },
          "telefone": {
            "pattern": "\\(\\d{2}\\)\\s\\d{4}-\\d{4}",
            "type": "string"
          },
          "paisDeOrigem": {
            "type": "string"
          },
          "endereco": {
