        "required": [
          "amenidades",
          "bairro",
          "cep",
          "cidade",
          "complemento",
          "endereco",
          "estado",
          "nome",
          "numero"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "nome": {
            "type": "string"
          },
          "amenidades": {
            "type": "string"
          },
          "endereco": {
            "type": "string"
          },
          "cep": {
            "type": "string"
          },
          "numero": {
            "type": "string"
          },
          "complemento": {
            "type": "string"
          },
